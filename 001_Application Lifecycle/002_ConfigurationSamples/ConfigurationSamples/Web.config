<configuration>

  <location path="Admin">
    <connectionStrings>
      <add name="AdminConnection" connectionString="DataSource=AdminDb ...."/>
    </connectionStrings>
  </location>

  <location path="User">
    <connectionStrings>
      <add name="UserConnection" connectionString="DataSource=UsersDb ...."/>
    </connectionStrings>
  </location>
  
  <system.web>
    <compilation debug="true" targetFramework="4.0" />
  </system.web>
</configuration>
